<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}


    </head>
    <body>
        <header class="container-md">
            <div class="row" id="titleBar"></div>
            <nav class="navbar-brand"></nav>

        </header>
        <main class="container-md">
            <div class="row">
                <div id="body" class="col-12 col-md-9 border">
                    {% block body %}Main{% endblock %}
                </div>
                <aside id="left_aside" class="col border">
                    {% block aside %}Aside{% endblock %}
                </aside>
            </div>
        </main>

        <footer class="container-md">
            Footer
        </footer>
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
